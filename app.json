{
  "expo": {
    "name": "BettyExpo",
    "slug": "BettyExpo",
    "scheme": "betty",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.betty.bettybooth",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "betty"
            ]
          }
        ]
      }
    },
    "android": {
      "package": "com.betty.bettybooth",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "*.bettybooth.com",
              "pathPrefix": "/gallery/"
            },
            {
              "scheme": "betty",
              "host": "gallery"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "eas": {
        "projectId": "83c00fa4-54eb-4f32-b95c-c772d3f5cd19"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/83c00fa4-54eb-4f32-b95c-c772d3f5cd19"
    }
  }
}
